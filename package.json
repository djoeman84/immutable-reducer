    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.4.0",
    "mocha": "^3.0.2",
    "redux-caller": "^1.1.9",
    "rimraf": "^2.5.2",
    "underscore": "^1.8.3"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "extendable-record": "0.0.2"
