  "license": "MIT",
  "description": "Immutable Reducers based off of ImmutableJS",
  "keywords": [
    "immutable",
    "immutablejs",
    "redux",
    "reducer",
    "object-oriented",
    "oo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/djoeman84/immutable-reducer.git"
  },
  "main": "dist/index.js",
  "scripts": {
